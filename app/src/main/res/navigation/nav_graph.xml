<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_main">

    <fragment
        android:id="@+id/nav_login"
        android:name="com.nogorsolutions.basis.ui.login.FragLogin"
        android:label="@string/menu_home"
        tools:layout="@layout/frag_login">
        <action
            android:id="@+id/action_nav_login_to_signUpFragment"
            app:destination="@id/nav_signup" />
    </fragment>

    <fragment
        android:id="@+id/nav_activity_main"
        android:name="com.nogorsolutions.basis.ui.login.FragLogin"
        android:label="@string/menu_home"
        tools:layout="@layout/activity_main">

    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.nogorsolutions.basis.ui.membership.MemberShipFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/frag_membership" />

    <fragment
        android:id="@+id/nav_resources"
        android:name="com.nogorsolutions.basis.ui.resources.ResourcesFragment"
        android:label="@string/menu_resources"
        tools:layout="@layout/frag_resources" />
    <fragment
        android:id="@+id/nav_home"
        android:name="com.nogorsolutions.basis.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/frag_home" />
    <fragment
        android:id="@+id/nav_main"
        android:name="com.nogorsolutions.basis.ui.main_view.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/frag_main" />
    <fragment
        android:id="@+id/nav_about_us"
        android:name="com.nogorsolutions.basis.ui.about.AboutFragment"
        android:label="About Us"
        tools:layout="@layout/frag_about" />
    <fragment
        android:id="@+id/nav_profile"
        android:name="com.nogorsolutions.basis.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/frag_profile" />
    <fragment
        android:id="@+id/nav_service"
        android:name="com.nogorsolutions.basis.ui.service.ServiceFrag"
        android:label="frag_service"
        tools:layout="@layout/frag_service">
        <action
            android:id="@+id/action_nav_service_to_generalServicesFragment"
            app:destination="@id/nav_generalServices" />
    </fragment>
    <fragment
        android:id="@+id/nav_generalServices"
        android:name="com.nogorsolutions.basis.ui.general_services.GeneralServicesFragment"
        android:label="General Services"
        tools:layout="@layout/frag_general_services" />
    <fragment
        android:id="@+id/nav_about_bangladesh_ictsector"
        android:name="com.nogorsolutions.basis.ui.about_bangladesh_ict_sector.AboutBangladeshICTSectorFragment"
        android:label="About Bangladesh ICT Sector"
        tools:layout="@layout/frag_about_bangladesh_ictsector" />
    <fragment
        android:id="@+id/nav_signup"
        android:name="com.nogorsolutions.basis.ui.sign_up.SignUpFragment"
        android:label="Sign Up"
        tools:layout="@layout/frag_sign_up" />
    <fragment
        android:id="@+id/ourInitiativesFragment"
        android:name="com.nogorsolutions.basis.ui.Initiatives.OurInitiativesFragment"
        android:label="OurInitiatives graph"
        tools:layout="@layout/fragment_our_initiatives" />




</navigation>